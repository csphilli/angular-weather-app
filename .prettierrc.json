{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": false,
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": false,
	"printWidth": 100,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"singleAttributePerLine": false,
	"htmlWhitespaceSensitivity": "ignore",
	"experimentalTernaries": true
}